<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="com.demoTMS.controller.BackOffice_MainView"
	xmlns:custom="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1" xmlns:html="http://www.w3.org/1999/xhtml">
	<SplitApp id="split" initialDetail="stu" initialMaster="master2" backgroundColor="White">
		<detailPages>
			<Page id="stu" title=" Student"  class="sapUiStdPage">
				<content>
					<mvc:XMLView viewName="com.demoTMS.view.BackOffice.addStudent" />
				</content>
			</Page>
			<Page id="stu1" title=" Student" class="sapUiStdPage" showNavButton="true" navButtonText="Back"
				navButtonPress="onPressDetailBack">
				<content>
					<mvc:XMLView viewName="com.demoTMS.view.BackOffice.viewStudent" />
				</content>
			</Page>
			<Page id="course1" title="Course" class="sapMPage" showNavButton="true" navButtonText="Back" navButtonPress="onPressDetailBack" enableScrolling="true">
				<content>
					<mvc:XMLView viewName="com.demoTMS.view.BackOffice.addCourse" />
				</content>
			</Page>
			<Page id="course2" title=" Course" class="sapMPage" showNavButton="true" navButtonText="Back" navButtonPress="onPressDetailBack" enableScrolling="true">
				<content>
					<mvc:XMLView viewName="com.demoTMS.view.BackOffice.viewCourse" />
				</content>
			</Page>
			<Page id="fac1" title=" BackOffice" class="sapMPage" showNavButton="true" navButtonText="Back" navButtonPress="onPressDetailBack" enableScrolling="true">
				<content>
					<mvc:XMLView viewName="com.demoTMS.view.BackOffice.addFacalty" />
				</content>
			</Page>
			
			<Page id="fac2" title=" BackOffice" class="sapMPage" showNavButton="true" navButtonText="Back" navButtonPress="onPressDetailBack" enableScrolling="true">
				<content>
					<mvc:XMLView viewName="com.demoTMS.view.BackOffice.viewFaculty" />
				</content>
			</Page>
			<Page id="add1" title=" Reminder" class="sapMPage" showNavButton="true" navButtonText="Back" navButtonPress="onPressDetailBack" enableScrolling="true">
				<content>
					<mvc:XMLView viewName="com.demoTMS.view.BackOffice.addReminder" />
				</content>
			</Page>
			<Page id="vwrmdr" title="View Reminder" class="sapMPage" showNavButton="true" navButtonText="Back" navButtonPress="onPressDetailBack" enableScrolling="true">
				<content>
					<mvc:XMLView viewName="com.demoTMS.view.BackOffice.View_Reminder" />
				</content>
			</Page>
			<Page id="stupymnt" title="Student Payment" class="sapMPage" showNavButton="true" navButtonText="Back" navButtonPress="onPressDetailBack" enableScrolling="true">
				<content>
					<mvc:XMLView viewName="com.demoTMS.view.BackOffice.student_payment" />
				</content>
			</Page>
			<Page id="vstupymnt" title="View Student Payment" class="sapMPage" showNavButton="true" navButtonText="Back" navButtonPress="onPressDetailBack" enableScrolling="true">
				<content>
					<mvc:XMLView viewName="com.demoTMS.view.BackOffice.viewStudent_Payment" />
				</content>
			</Page>
			<Page id="fclpymnt" title="Faculty Payment" class="sapMPage" showNavButton="true" navButtonText="Back" navButtonPress="onPressDetailBack" enableScrolling="true">
				<content>
					<mvc:XMLView viewName="com.demoTMS.view.BackOffice.Facalty_Payment" />
				</content>
			</Page>
			<Page id="vfclpymnt" title="View Faculty Payment" class="sapMPage" showNavButton="true" navButtonText="Back" navButtonPress="onPressDetailBack" enableScrolling="true">
				<content>
					<mvc:XMLView viewName="com.demoTMS.view.BackOffice.viewFacalty_Payment" />
				</content>
			</Page>
		</detailPages>
		<masterPages>
			<Page id="master2" showHeader="true" title="BackOffice" class="sapUiStdPage" showNavButton="false" navButtonPress="onPressMasterBack">
				<content>
					<Tree id="Tree" items="{path: 'fac>/'}" selectionChange="onListItemPress" mode="SingleSelectMaster" includeItemInSelection="true">
						<StandardTreeItem title="{fac>text}" icon="{fac>ref}"  type="Active" custom:to="{fac>custom}"/>
					</Tree>
				</content>
			</Page>
		</masterPages>
	</SplitApp>
</mvc:View>